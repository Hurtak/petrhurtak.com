{
  "ci": {
    "collect": {
      "staticDistDir": "./dist",
      "maxAutodiscoverUrls": 100
    },
    "assert": {
      "assertions": {
        "categories:performance": ["error", { "minScore": 0.95 }],
        "categories:accessibility": ["error", { "minScore": 1 }],
        "categories:bestPractices": ["error", { "minScore": 1 }],
        "categories:seo": ["error", { "minScore": 0.85 }]
      }
    }
  }
}
