{
  "name": "petrhurtak.com",
  "version": "1.0.0",
  "repository": "github:hurtak/petrhurtak.com",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "npm run generate-rss && vite build",
    "preview": "vite preview",
    "generate-rss": "tsx scripts/generate-rss.ts",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "type-check": "tsc --noEmit -p tsconfig.app.json && tsc --noEmit -p tsconfig.test.json",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix .",
    "prettier": "prettier --ignore-path .gitignore --check .",
    "prettier:fix": "prettier --ignore-path .gitignore --write .",
    "validate": "npm run test && npm run type-check && npm run lint && npm run prettier"
  },
  "dependencies": {
    "@types/node": "20.12.4",
    "@types/ramda": "0.29.12",
    "@types/react": "18.2.74",
    "@types/react-dom": "18.2.22",
    "clsx": "2.1.0",
    "dayjs": "1.11.10",
    "feed": "4.2.2",
    "normalize.css": "8.0.1",
    "ramda": "0.29.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-ga4": "2.1.0",
    "react-router-dom": "6.23.1",
    "react-lite-youtube-embed": "2.4.0",
    "react-refractor": "2.1.7",
    "refractor": "4.8.1",
    "styled-jsx": "5.1.2",
    "strip-indent": "4.0.0",
    "typescript": "5.4.3",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/react-test-renderer": "18.0.7",
    "@vitejs/plugin-react": "4.2.1",
    "eslint": "8.57.0",
    "eslint-config-start": "2.0.0",
    "eslint-config-start-react": "2.1.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "prettier": "3.2.5",
    "react-test-renderer": "18.2.0",
    "tsx": "4.7.1",
    "vite": "5.2.8",
    "vitest": "1.4.0"
  }
}
