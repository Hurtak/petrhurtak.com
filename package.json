{
  "name": "petrhurtak.com",
  "version": "1.0.0",
  "repository": "github:hurtak/petrhurtak.com",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export -o dist",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "type-check": "tsc --noEmit -p tsconfig.next.json && tsc --noEmit -p tsconfig.test.json",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix .",
    "prettier": "prettier --ignore-path .gitignore --check .",
    "prettier:fix": "prettier --ignore-path .gitignore --write .",
    "validate": "npm run test && npm run type-check && npm run lint && npm run prettier"
  },
  "dependencies": {
    "clsx": "1.1.1",
    "dayjs": "1.11.2",
    "feed": "4.2.2",
    "next": "12.1.6",
    "next-videos": "1.4.0",
    "normalize.css": "8.0.1",
    "ramda": "0.28.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-ga4": "1.4.1",
    "react-refractor": "2.1.7",
    "refractor": "4.7.0",
    "strip-indent": "4.0.0",
    "zod": "3.17.3"
  },
  "devDependencies": {
    "@types/node": "17.0.39",
    "@types/ramda": "0.28.13",
    "@types/react": "18.0.11",
    "@types/react-test-renderer": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "prettier": "2.6.2",
    "react-test-renderer": "18.1.0",
    "typescript": "4.7.3",
    "vitest": "0.13.1"
  }
}
