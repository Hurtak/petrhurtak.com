{
  "name": "petrhurtak.com",
  "version": "1.0.0",
  "repository": "github:hurtak/petrhurtak.com",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export -o dist",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "type-check": "tsc --noEmit -p tsconfig.next.json && tsc --noEmit -p tsconfig.test.json",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix .",
    "prettier": "prettier --ignore-path .gitignore --check .",
    "prettier:fix": "prettier --ignore-path .gitignore --write .",
    "validate": "npm run test && npm run type-check && npm run lint && npm run prettier"
  },
  "dependencies": {
    "@types/node": "18.6.2",
    "@types/ramda": "0.28.15",
    "@types/react": "18.0.15",
    "clsx": "1.2.1",
    "dayjs": "1.11.4",
    "feed": "4.2.2",
    "next": "12.2.3",
    "next-videos": "1.4.0",
    "normalize.css": "8.0.1",
    "ramda": "0.28.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-ga4": "1.4.1",
    "react-refractor": "2.1.7",
    "refractor": "4.7.0",
    "strip-indent": "4.0.0",
    "typescript": "4.7.4",
    "zod": "3.17.10"
  },
  "devDependencies": {
    "@types/react-test-renderer": "18.0.0",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.3",
    "eslint-config-start": "1.1.0",
    "eslint-config-start-react": "2.0.0",
    "prettier": "2.7.1",
    "react-test-renderer": "18.2.0",
    "vitest": "0.19.1"
  }
}
