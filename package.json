{
  "name": "petrhurtak.com",
  "version": "1.0.0",
  "repository": "github:hurtak/petrhurtak.com",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export -o dist",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "type-check": "tsc --noEmit -p tsconfig.next.json && tsc --noEmit -p tsconfig.test.json",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix .",
    "prettier": "prettier --ignore-path .gitignore --check .",
    "prettier:fix": "prettier --ignore-path .gitignore --write .",
    "validate": "npm run test && npm run type-check && npm run lint && npm run prettier"
  },
  "dependencies": {
    "@types/node": "20.6.3",
    "@types/ramda": "0.29.4",
    "@types/react": "18.2.22",
    "clsx": "2.0.0",
    "dayjs": "1.11.10",
    "feed": "4.2.2",
    "next": "13.5.4",
    "next-videos": "1.5.0",
    "normalize.css": "8.0.1",
    "ramda": "0.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-ga4": "2.1.0",
    "react-lite-youtube-embed": "2.3.52",
    "react-refractor": "2.1.7",
    "refractor": "4.8.1",
    "strip-indent": "4.0.0",
    "typescript": "4.9.5",
    "zod": "3.22.3"
  },
  "devDependencies": {
    "@types/react-test-renderer": "18.0.0",
    "eslint": "8.34.0",
    "eslint-config-next": "13.2.1",
    "eslint-config-start": "1.1.3",
    "eslint-config-start-react": "2.0.1",
    "prettier": "2.8.4",
    "react-test-renderer": "18.2.0",
    "vitest": "0.26.2"
  }
}
